    "directory": "packages/typescript-utils"
  },
  "bugs": {
    "url": "https://github.com/outfitter-dev/camp/issues"
  },
  "homepage": "https://github.com/outfitter-dev/camp/tree/main/packages/typescript-utils#readme"
